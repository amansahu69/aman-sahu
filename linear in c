#include<stdio.h>

int main(){
    int a[20],r, search,k=0,i;
    printf("enter th range=");
    scanf("%d",&r);

    printf("enter a %d numbar",r);
    for(int i=0;i<r;i++){
        scanf("%d",&a[i]);
    }
    printf("enter a number you want to search=");        
    scanf("%d",&search);

     for(int i=0;i<r;i++){
        if(a[i]==search){

            printf("number is a  found %d at position ",i+1);
            k++;
            break;
            
        }
        if(k==0){
            printf("number is not found");
            
             
            
            
        }
     }

    return 0;
}
